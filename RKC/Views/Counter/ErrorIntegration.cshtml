@model IEnumerable<DB.Model.IntegrationReadings>
@{
    ViewBag.Title = "ErrorIntegration";
}
<h2>Ошибка интеграции</h2>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Лицевой счет</th>
            <th scope="col">Тип ПУ</th>
            <th scope="col">Дата</th>
            <th scope="col">Описание ошибки</th>
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var Items in Model)
        {
            <tr>
                <td>@Items.Lic</td>
                <td>@Items.TypePu</td>
                <td>@Items.DateTime</td>
                <td>@Items.Description</td>
                <td><a class="btn btn-danger" target="_blank" href="/Counter/DetailedInformIPU?FULL_LIC=@Items.Lic" >Показать</a></td>
            </tr>
        }
    </tbody>
</table>

