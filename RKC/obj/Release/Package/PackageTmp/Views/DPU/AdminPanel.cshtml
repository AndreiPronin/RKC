<div style="margin-top:15px">
    @using (Html.BeginForm("UploadFilePU", "DPU", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        if (User.IsInRole("Admin"))
        {
            <div style="margin-top:30px;" class="row">
                <div class="col-md-4">
                    @Html.TextBox("file", "", new { type = "file", @class = "form-control" })
                </div>
                <div class="col-md-6">
                    <select name="TypeLoad" id="TypeLoad" class="form-control">
                        <option value="1">Установка ДПУ установка</option>
                        <option value="2">Сумма ДПУ </option>
                    </select>
                    @*<input name="Type" id="Type" class="form-control" />*@
                </div>
                <div class="col-md-2">
                    <input type="submit" id="" class="btn btn-success" value="Загрузить" />
                </div>
            </div>
        }
    }
</div>
