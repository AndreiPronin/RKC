@model IEnumerable<DB.Model.ALL_LICS>
@{int i = 1;}
<p>@ViewBag.Count</p>
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Номер лицевого счета</th>
            <th scope="col">Адрес</th>
            <th scope="col">ФИО</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var Items in Model)
        {
            <tr>
                <th scope="row">@i</th>
                <td>@Items.F4ENUMELS</td>
                <td>
                    @{if (Items.UL?.Length > 0)
                        { <span>улица @Items.UL</span>;
                    } }
                    @{if (Items.DOM?.Length > 0)
                        { <span>дом @Items.DOM</span>;
                    } }
                    @{if (Items.KW?.Length > 0)
                        { <span>кв. @Items.KW</span>;
                    } }
                </td>
                 <td>@Items.FIO</td>
                <td>
                    <a href="/counter/DetailedInformIPU?FULL_LIC=@Items.F4ENUMELS" target="_blank" class="btn btn-primary">
                        Показать подробную информацию
                    </a></td>
            </tr>
            i++;
        }
    </tbody>
</table>


