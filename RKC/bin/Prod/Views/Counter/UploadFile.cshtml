@model IEnumerable<BE.Service.ReadFilesModel>
   
<div class="container">
    <h3>Результат обработки файла</h3>
    <table class="table table-dark">
        <thead>
            <tr>
                <th scope="col">Лицевой счет</th>
                <th scope="col">Тип пу</th>
                <th scope="col">Конечные показания</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var Items in Model)
            {
                <tr>
                    <td>@Items.FullLic</td>
                    <td style="white-space: break-spaces ">@Items.TypePU</td>
                    <td>@Items.Indications</td>
                </tr>
            }



        </tbody>
    </table>
</div>